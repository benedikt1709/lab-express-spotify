<table class="table table-dark">
    <thead>
        <tr>
            <th scope="col">Track-No.</th>
            <th scope="col">Title</th>
        </tr>
    </thead>
    {{#each allSongs}}
    <tbody>
        <tr>
            <th scope="row">{{this.track_number}}</th>
            <td>{{this.name}}</td>
            <td>
                <audio controls>
                    <source src="{{this.preview_url}}" type="audio/mpeg">
                    Your browser does not support the audio tag.
                </audio>
            </td>
        </tr>

    </tbody>
    {{> songsPartial this}}
    {{/each}}
</table>